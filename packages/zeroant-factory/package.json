{
  "name": "zeroant-factory",
  "version": "1.0.0-alpha.18",
  "description": "Factory modules for zeroant",
  "repository": "git@github.com:michaelpiper/zeroant-factory.git",
  "main": "lib/index.js",
  "type": "module",
  "scripts": {
    "lint": "eslint ./src/* --fix",
    "build": "rm -rf ./lib && tsc -p tsconfig.json",
    "prepublish": "npm run lint && npm run build"
  },
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts",
      "require": "./lib/index.js"
    },
    "./*": "./lib/*.js",
    "./**/*": "./lib/**/*.js"
  },
  "keywords": [
    "koa",
    "ts",
    "KISS",
    "SOLID",
    "DRY"
  ],
  "author": "Michael Piper",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^8.1.0",
    "bull": "^4.10.4",
   
    "koa-router": "^12.0.0",
    "winston": "^3.8.2"
  },
  "peerDependencies": {
    "koa": "^2.14.2",
    "zeroant-constant": "^1.0.0-alpha.2",
    "zeroant-response": "^1.0.0-alpha.2"
  },
  "devDependencies": {
    "eslint": "^8.40.0",
    "@types/koa": "^2.13.9",
    "@types/koa-router": "^7.4.4",
    "@types/node": "^20.8.0",
    "typescript": "^5.0.4"
  },
  "gitHead": "328dda5e11a02b5b5385e04cfdfde6712dab41cd"
}
