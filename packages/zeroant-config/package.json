{
  "name": "zeroant-config",
  "version": "1.0.0-alpha.18",
  "description": "config modules for zeroant",
  "repository": "git@github.com:michaelpiper/zeroant-config.git",
  "main": "lib/index.js",
  "type": "module",
  "scripts": {
    "lint": "eslint ./src/* --fix",
    "build": "rm -rf ./lib && tsc -p tsconfig.json",
    "prepublish": "npm run lint && npm run build"
  },
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts",
      "require": "./lib/index.js"
    },
    "./*": "./lib/*.js",
    "./**/*": "./lib/**/*.js"
  },
  "keywords": [
    "koa",
    "ts",
    "KISS",
    "SOLID",
    "DRY"
  ],
  "author": "Michael Piper",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.0.3"
  },
  "peerDependencies": {
    "zeroant-factory": "^1.0.0-alpha.0"
  },
  "devDependencies": {
    "eslint": "^8.40.0",
    "@types/dotenv": "^8.2.0",
    "@types/node": "^20.8.0",
    "typescript": "^5.0.4"
  },
  "gitHead": "6ab97eb709cc1c7ed19433c207f558b4e582dd69"
}
