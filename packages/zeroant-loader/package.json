{
  "name": "zeroant-loader",
  "version": "1.0.0-alpha.28",
  "description": "loader modules for zeroant",
  "repository": "git@github.com:michaelpiper/zeroant-loader.git",
  "main": "lib/index.js",
  "type": "module",
  "scripts": {
    "lint": "eslint ./src/* --fix",
    "build": "rm -rf ./lib && tsc -p tsconfig.json",
    "prepublish": "npm run lint && npm run build"
  },
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts",
      "require": "./lib/index.js"
    },
    "./*": "./lib/*.js",
    "./**/*": "./lib/**/*.js"
  },
  "keywords": [
    "koa",
    "ts",
    "KISS",
    "SOLID",
    "DRY"
  ],
  "author": "Michael Piper",
  "license": "ISC",
  "peerDependencies": {
    "zeroant-config": "^1.0.0-alpha.1",
    "zeroant-factory": "^1.0.0-alpha.0",
    "zeroant-logger": "^1.0.0-alpha.1"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "eslint": "^8.40.0",
    "typescript": "^5.0.4",
    "zeroant-config": "^1.0.0-alpha.28",
    "zeroant-factory": "^1.0.0-alpha.28",
    "zeroant-logger": "^1.0.0-alpha.28"
  },
  "gitHead": "eb33e879f773af06a1b91e03b25089f31cbaa582"
}
